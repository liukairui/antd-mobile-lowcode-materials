{
  "name": "antd-mobile-lowcode-materials",
  "version": "0.1.10",
  "description": "Low-code Materials based on Antd Mobile",
  "license": "MIT",
  "author": "Kyrix",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "directories": {
    "lib": "lib",
    "doc": "docs"
  },
  "files": [
    "build",
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "build-scripts build",
    "start": "build-scripts start --config ./build.lowcode.js",
    "lowcode:build": "build-scripts build --config ./build.lowcode.js",
    "f2elint-fix": "f2elint fix",
    "f2elint-scan": "f2elint scan",
    "repl": "npm publish --force --registry https://registry.npmjs.org/",
    "prepublishOnly": "npm run build && npm run lowcode:build",
    "sort": "node ./scripts/sort.js && npx prettier --write ."
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": "f2elint exec eslint",
    "**/*.{css,scss,less,acss}": "f2elint exec stylelint"
  },
  "resolutions": {
    "webpack": "4.46.x"
  },
  "dependencies": {
    "@alifd/next": "1.26.22",
    "antd-mobile": "5.32.0",
    "antd-mobile-icons": "0.3.0",
    "moment": "2.29.4",
    "pinyin": "2.11.0",
    "prop-types": "15.8.1",
    "rc-table": "7.32.1",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@alib/build-scripts": "0.1.32",
    "@alifd/build-plugin-lowcode": "0.3.18",
    "@alifd/theme-2": "0.4.4",
    "@alilc/build-plugin-alt": "1.3.3",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/source-loader": "6.5.16",
    "@types/pinyin": "2.10.0",
    "@types/react": "16.14.43",
    "@types/react-dom": "16.9.19",
    "build-plugin-component-multiple": "1.0.0-beta.53",
    "build-plugin-fusion": "0.1.23",
    "f2elint": "1.2.3",
    "typescript": "5.1.6"
  },
  "acceptDependencies": {
    "webpack": "4.46.x"
  },
  "componentConfig": {
    "isComponentLibrary": true,
    "materialSchema": "https://unpkg.com/antd-mobile-lowcode-materials@0.1.10/build/lowcode/assets-prod.json"
  }
}